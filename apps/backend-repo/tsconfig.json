{
  "compilerOptions": {
    "target": "ES6",
    "module": "CommonJS",
    "outDir": "dist",
    "rootDir": "./",
    "strict": false,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "noEmitOnError": false,
    "noImplicitAny": false,
    "strictNullChecks": false,
    "strictPropertyInitialization": false
  },
  "include": [
    "**/*.ts"
  ],
  "exclude": [
    "node_modules", 
    "firebase-debug.log", 
    "firebase-debug.*.log"
  ]
}